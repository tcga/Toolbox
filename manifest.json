{
    "manifest_version": 2,
    "name": "TCGA Toolbox",
    "description": "A collection of tools to process and analyze files from TCGA's open-access HTTP directory directly in the browser.",
    "version": "0.11.1",
    "app": {
        "launch": {
            "local_path": "main.html"
        }
    },
    "permissions": [
        "http://tcga-data.nci.nih.gov/",
        "https://tcga-data.nci.nih.gov/",
        "unlimitedStorage"
    ],
    "content_security_policy": "script-src 'self' https://; object-src 'self'",
    "web_accessible_resources": [
        "img/installed.png"
    ],
    "icons": {
        "128": "icon_128.png",
        "16": "icon_16.png"
    }
}
